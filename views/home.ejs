<%- include('partials/header') %>

<h1>Home</h1>
<p><%= homeStartingContent %></p>

<% foundPost.forEach(function(post) { %>
    <h2><%= post.title %></h2>
    <p>
        <%= post.content.substring(0, 100) + '...' %>
        <a href="/posts/<%= post._id %>">READ MORE</a>
    </p>
<% }); %>


<%- include('partials/footer') %>